@model OffshoreTrack.Models.Permissao

@{
    ViewData["Title"] = "Read";
}

<div class="pagetitle">
    <div class="row">
        <div class="col-md-6">
            <h1 class="display-4">Visualização: Permissão</h1>
        </div>
        <div class="col-md-6">
            <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                <a class="btn btn-primary" role="button" asp-area="" asp-controller="Permissao" asp-action="Index">Voltar</a>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <label for="inputText" class="col-sm col-form-label">Permissão:</label>
                <input type="text" class="form-control" id="nome_permissao" name="nome_permissao" asp-for="nome_permissao" readonly />
                <br>
                <dl class="row">
                    <dt class="col-sm-2">Pode Criar</dt>
                    <dd class="col-sm-10">  @Html.DisplayFor(model => model.pode_criar)</dd>
                    <br>
                    <dt class="col-sm-2">Pode Ler</dt>
                    <dd class="col-sm-10">  @Html.DisplayFor(model => model.pode_ler)</dd>
                    <br>
                    <dt class="col-sm-2">Pode Atualizar</dt>
                    <dd class="col-sm-10">@Html.DisplayFor(model => model.pode_atualizar)</dd>
                    <br>
                    <dt class="col-sm-2">Pode Deletar</dt>
                    <dd class="col-sm-10">@Html.DisplayFor(model => model.pode_deletar)</dd>
                </dl>
                <br />
            </div>
        </div>
    </div>
</div>
    

